{
  "name": "private-reserve",
  "version": "1.0.0",
  "description": "A private reserve system",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test sequelize db:migrate:undo:all && npm run db:migrate:test && npm run db:migrate:seed:test && NODE_ENV=test nyc mocha --require @babel/register --timeout 30000 src/tests/index.js",
    "cover": "npm test && nyc report --reporter=text-lcov | coveralls",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "start": "concurrently \"npm run db:migrate && npm run db:seed\" \"babel-node src/index.js\"",
    "dev": "concurrently \"npm run db:seed\"  \"nodemon src/index.js --exec babel-node --presets @babel/preset-env\"",
    "scss": "npm-sass templates/sass/style.scss templates/css/style.css",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo:all:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@hapi/joi": "^17.0.0",
    "@sendgrid/mail": "^6.4.0",
    "bcrypt": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.19.0",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-easy-query": "^1.1.0",
    "sequelize-test-helpers": "^1.2.2",
    "socket.io": "^2.3.0",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3",
    "yc": "^0.0.2"
  }
}
